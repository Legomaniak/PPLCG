<Window x:Class="CardFiller.MyListWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MyListWindow" Height="300" Width="300" xmlns:my="clr-namespace:CardFiller">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100" />
            <ColumnDefinition Width="100*" />
        </Grid.ColumnDefinitions>
        <my:MyListControl x:Name="myListControl1" Grid.Column="1" />
        <Label Content="Label" Height="28" Name="label1" VerticalAlignment="Top" />
        <Button Content="Ok" Height="23" Name="button1" VerticalAlignment="Bottom" Click="clickOk"/>
    </Grid>
</Window>
