<Window x:Class="CardFiller.KartaOkno"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="KartaOkno" Height="400" Width="300" xmlns:my="clr-namespace:CardFiller">
    <Grid>
        <GroupBox x:Name="karta" Header="Karta" Margin="0,0,0,28"/>
                  
        <Button Content="Ok" Height="23" HorizontalAlignment="Left" Margin="0,0,0,0" Name="buttonOk" VerticalAlignment="Bottom" Width="100" IsDefault="True" Click="buttonOk_Click" />
        <Button Content="Zrušit" Height="23" HorizontalAlignment="Right" x:Name="buttonOk_Copy" VerticalAlignment="Bottom" Width="100" IsCancel="True" />
    </Grid>
</Window>
